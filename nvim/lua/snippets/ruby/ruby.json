{
  "Tap debugger": {
    "prefix": "dbg",
    "body": [
      "tap { |obj| debugger }"
    ]
  },
  "Debugger if": {
    "prefix": "dbi",
    "body": [
      "debugger if ${0:Debug.debug?}"
    ]
  },
  "Frozen string literal": {
    "prefix": "fsl",
    "body": [
      "# frozen_string_literal: true",
      "",
      "${0}"
    ]
  }
}
